pProblem(\"Maksymalizacja_zysku\", pulp.LpMaximize)\n","\n","    # Zmienne decyzyjne\n","    m = pulp.LpVariable(\"Mars\", lowBound=0, cat='Continuous')\n","    s = pulp.LpVariable(\"Snickers\", lowBound=0, cat='Continuous')\n","\n","    # Koszt produkcji 1 kg Marsa\n","    km = 0.4 * kcz + 0.2 * kk + 0.05 * kol + 0.3 * kcuk\n","    # Koszt produkcji 1 kg Snickersa\n","    ks = 0.4 * kcz + 0.2 * kor + 0.05 * kol + 0.2 * kk + 0.3 * kcuk\n","\n","    # Funkcja celu (maksymalizacja zysku)\n","    model += (c_mars - km) * m + (c_sn - ks) * s, \"Zysk\"\n","\n","    # Ograniczenia\n","    model += m * 0.5 + s * 0.4 <= wyd_czek, \"Ograniczenie na czekoladę\"\n","    model += 0.2 * s <= wyd_orz, \"Ograniczenie na orzechy\"\n","    model += m + s <= wyd_pak, \"Ograniczenie na pakowanie\"\n","\n","    # Rozwiązanie modelu\n","    model.solve()\n","\n","    # Sprawdzamy status rozwiązania\n","    status = pulp.LpStatus[model.status]\n","\n","    if status == \"Optimal\": #warunek czy status jest optymalny\n","        zuzyta_czekolada = m.varValue * 0.5 + s.varValue * 0.4\n","        zuzyte_orzechy = s.varValue * 0.2\n","        print(f\"Optymalna ilość Marsów: {m.varValue} kg\")\n","        print(f\"Optymalna ilość Snickersów: {s.varValue} kg\")\n","        print(f\"Zużyta czekolada: {zuzyta_czekolada} kg\")\n","        print(f\"Zużyte orzechy: {zuzyte_orzechy} kg\")\n","        print(f\"Maksymalny zysk: {pulp.value(model.objective)} zł\")\n","    else:\n","        print(f\"Rozwiązanie nie jest optymalne. Status: {status}\")\n","    #x = np.linspace(200, 100, 5000)\n","\n","    #y1 = (pulp.value(model.objective))\n","\n","# Make plot\n","   # plt.plot(x, y1, label=r'$1.2*x_1+2*x_2 <= 180$')\n","   # plt.xlim((0, 5000))\n","    #plt.ylim((0, 2500000))\n","   # plt.xlabel(r'$x_1$')\n","\n","\n","# Teraz przekazujesz funkcję produkcja_batonow jako argument do interact:\n","interact(produkcja_batonow, kcz=koszt_czekolady_slider, kk=koszt_karmelu_slider,\n","         kcuk=koszt_cukru_slider, kol=koszt_oleju_slider, kor=koszt_orzechow_slider,\n","         wyd_orz=wydajnosc_orzecharki_slider, wyd_pak=wydajnosc_pakowarki_slider,\n","         wyd_czek=wydajnosc_czekoladarki_slider,\n","         c_sn = cena_snickers_slider, c_mars = cena_mars_slider)\n","\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":533,"referenced_widgets":["9010a1dd930048eb967277b7cd46f253","9d1f8752d96145ef9b99cb4e2d73a454","196e28a2892940bfb186716d9538c395","4084d58184804b25ae7d3fcc6801e4da","67ef9f704ca0400aaa92e464f26730c1","2c98c7bc2c1440f2a5b88eab10fcce5a","f8d766b8670243f38063ad811289a573","d43e69ffb51f4c10afc4fd79648dcfa2","c60c481018f8440896cd84f8c149cc23","65827be34fbc41a6a25ef03137a7814b","88f5aa53c8f5495e911535f4a446fe7d","f854751e3da440b5919295dd1517745a","f662a796d95b49db902eaa9c1490c60d","de86ace7effc43c081a7ac10149c94bc","0a24186707fd4f9689e6c2453b3f4df5","976ec1ae96ba4dea8ecb0576fb41b344","e2bd1d990a0b4d689d9d97c82d1f2416","ab956eee316445bca81605f00128f7ae","5378723f0c9e41db8ea750f72bea2581","fab7e8d5c6da4211841d56dcb4514f77","eecc4f835bda47d197618be87478b9c1","dfdd9f43c2b34952abf93da529c66f27","591c9f24a59246aa801c1a892e0abd1e","46643868038a473c8aaa86b5f63b10b8","6eddaffd93a0418190655cd7480ccede","241db720b076496b95947683fffb4ffe","c26dc5248712473f97543a1d613e3f23","865edca712974fd2967d7b582fa93c84","fccf43f572954de1bb6103fc0251f75d","bcbd460c5a78407aa9f573329ab83644","47b2bb9e5f1d45d696841c07125a846b","8932c293736140a7aef5f5d1b9b390b9","e614ab4cafd548b586e3c972b83dc102","8635cb6e2e2a4379aa9a04feee8c1357"]},"id":"gaujXs5GCIoF","executionInfo":{"status":"ok","timestamp":1730204816959,"user_tz":-60,"elapsed":337,"user":{"displayName":"Marek Fiut","userId":"16357055783240005496"}},"outputId":"870faafc-710a-4f57-c4df-a74de9d8e31d"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["interactive(children=(FloatSlider(value=20.0, description='koszt_czekolady_slider', max=40.0, min=15.0, style=…"],"appl
ja się udała\n","\n","#print(pulp.___version___)\n","pulp.pulpTestAll()\n","%matplotlib inline\n","\n","style = {'description_width': 'initial'}\n","\n","\n","koszt_czekolady_slider = FloatSlider(min=15,max = 40, value= 20, description=\"koszt_czekolady_slider\", style= style )\n","koszt_karmelu_slider = FloatSlider(min=10,max = 20,value= 15, description=\"koszt_karmelu_slider\", style= style )\n","koszt_oleju_slider = FloatSlider(min=4,max = 12, value= 7, description=\"koszt_oleju_slider\", style= style )\n","koszt_cukru_slider = FloatSlider(min=2,max = 6,value= 4, description=\"koszt_cukru_slider\", style= style )\n","koszt_orzechow_slider = FloatSlider(min=15,max = 30, value= 20, description=\"koszt_orzechow_slider\", style= style )\n","wydajnosc_orzecharki_slider = FloatSlider(min=500,max = 5000,value= 2000, description=\"wydajnosc_orzecharki_slider\", style= style )\n","wydajnosc_pakowarki_slider = FloatSlider(min=8000,max = 15000,value= 10000, description=\"wydajnosc_pakowarki_slider\", style= style )\n","wydajnosc_czekoladarki_slider = FloatSlider(min=5000,max = 7500,value= 6000, description=\"wydajnosc_czekoladarki_slider\", style= style )\n","cena_mars_slider = FloatSlider(min=90,max = 150, value= 100, description=\"cena_mars_slider\", style= style )\n","cena_snickers_slider = FloatSlider(min=90,max = 150,value= 105, description=\"cena_snickers_slider\", style= style )\n","\n","\n","# Tworzymy problem maksymalizacji zysku\n","\n","def produkcja_batonow(kcz, kk=15, kol=7, kor=20, kcuk=4, wyd_orz = 2000, wyd_pak = 10000, wyd_czek = 6000, c_mars = 100, c_sn = 105):\n","    model = pulp.LpProblem(\"Maksymalizacja_zysku\", pulp.LpMaximize)\n","\n","    # Zmienne decyzyjne\n","    m = pulp.LpVariable(\"Mars\", lowBound=0, cat='Continuous')\n","    s = pulp.LpVariable(\"Snickers\", lowBound=0, cat='Continuous')\n","\n","    # Koszt produkcji 1 kg Marsa\n","    km = 0.4 * kcz + 0.2 * kk + 0.05 * kol + 0.3 * kcuk\n","    # Koszt produkcji 1 kg Snickersa\n","    ks = 0.4 * kcz + 0.2 * kor + 0.05 * kol + 0.2 * kk + 0.3 * kcuk\n","\n","    # Funkcja celu (maksymalizacja zysku)\n","    model += (c_mars - km) * m + (c_sn - ks) * s, \"Zysk\"\n","\n","    # Ograniczenia\n","    model += m * 0.5 + s * 0.4 <= wyd_czek, \"Ograniczenie na czekoladę\"\n","    model += 0.2 * s <= wyd_orz, \"Ograniczenie na orzechy\"\n","    model += m + s <= wyd_pak, \"Ograniczenie na pakowanie\"\n","\n","    # Rozwiązanie modelu\n","    model.solve()\n","\n","    # Sprawdzamy status rozwiązania\n","    status = pulp.LpStatus[model.status]\n","\n","    if status == \"Optimal\":\n","        zuzyta_czekolada = m.varValue * 0.5 + s.varValue * 0.4\n","        zuzyte_orzechy = s.varValue * 0.2\n","        print(f\"Optymalna ilość Marsów: {m.varValue} kg\")\n","        print(f\"Optymalna ilość Snickersów: {s.varValue} kg\")\n","        print(f\"Zużyta czekolada: {zuzyta_czekolada} kg\")\n","        print(f\"Zużyte orzechy: {zuzyte_orzechy} kg\")\n","        print(f\"Maksymalny zysk: {pulp.value(model.objective)} zł\")\n","\n","        # Tworzenie wykresu\n","        wydajnosci_orzecharki = np.linspace(500, 5000, 50)\n","        zyski = []\n","        for wydajnosc in wydajnosci_orzecharki:\n","            model_temp = pulp.LpProblem(\"Maksymalizacja_zysku_temp\", pulp.LpMaximize)\n","            m_temp = pulp.LpVariable(\"Mars_temp\", lowBound=0, cat='Continuous')\n","            s_temp = pulp.LpVariable(\"Snickers_temp\", lowBound=0, cat='Continuous')\n","            model_temp += (c_mars - km) * m_temp + (c_sn - ks) * s_temp, \"Zysk_temp\"\n","            model_temp += m_temp * 0.5 + s_temp * 0.4 <= wyd_czek, \"Ograniczenie_na_czekoladę_temp\"\n","            model_temp += 0.2 * s_temp <= wydajnosc, \"Ograniczenie_na_orzechy_temp\"\n","            model_temp += m_temp + s_temp <= wyd_pak, \"Ograniczenie_na_pakowanie_temp\"\n","            model_temp.solve()\n","            if pulp.LpStatus[model_temp.status] == \"Optimal\":\n","                zyski.append(pulp.value(model_temp.objective))\n","            else:\n","                zyski.append(0)  # lub inna wartość w przypadku braku rozwiązania optymalnego\n","\n","        plt.figure(figsize=(10, 6))\n","        plt.plot(wydajnosci_orzecharki, zyski)\n","        plt.xlabel(\"Wydajność orzecharki (kg)\")\n","        plt.ylabel(\"Zysk (zł)\")\n","        plt.title(\"Wpływ wydajności orzecharki na zysk\")\n","        plt.grid(True)\n","        plt.show()\n","\n","    else:\n","        print(f\"Rozwiązanie nie jest optymalne. Status: {status}\")\n","\n","# Teraz przekazujesz funkcję produkcja_batonow jako argument do interact:\n","interact(produkcja_batonow, kcz=koszt_czekolady_slider, kk=koszt_karmelu_slider,\n","         kcuk=koszt_cukru_slider, kol=koszt_oleju_slider, kor=koszt_orzechow_slider,\n","         wyd_orz=wydajnosc_orzecharki_slider, wyd_pak=wydajnosc_pakowarki_slider,\n","         wyd_czek=wydajnosc_czekoladarki_slider,\n","         c_sn = cena_snickers_slider, c_mars = cena_mars_slider)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["234af96132ab42d78416702e3e12cef5","39de2e7b3b924e36a5f7878b7bbd62b9","e2f6c7d7d1724e6c901338234f872896","178f57b74a8b4a3ebd5e37e9074b47c0","9ff5be95ee9c46f7906e25df5eea9f16","1ab3389bb144486f8a0d9812028d8dca","006eec8911c14ad1ae2b030caf5f811d","194c4e83b66949ebb7963c698bc8871d","ccf2859ac4d1475a952f855624db0abc","7822209a702448e3955c7db3a174b313","338cc9c5388349318d3341a7e4cb8b08","56b828eaa7d7428396da4c3467b744ff","02bfc30ccfa446cf93f714abd706f8e3","e4782f74dab34d2c9121cd27bb6296ce","7d0f49856f7b4883a79510cfe9830a63","a348c9e843c24ce7a94aa97896bc994a","8be18bb8c22c4942bc8f93260e2dcb44","a3bce0f4a11a46b4a4b53983da29484f","36b23c0d9c4b45758addfc22b9a05485","2ab49cb7dfb34b088d57dd9dca928cb5","9dc0fe69b6e6432b8b2b50f80402b3c2","17d0e0d2b4af45b4979b8f054b5a864f","1b48eab773f34d698b4c60c1a71b9c08","80498c706b6248778a92ab3a72242a88","07257534933547ec8e406179441cb7c7","fec071fcdeee4673b23